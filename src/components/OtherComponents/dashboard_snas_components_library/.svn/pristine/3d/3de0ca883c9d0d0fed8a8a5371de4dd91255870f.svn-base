.dark-light-mode-toggle {
  position: relative;
  width: 50px;
  height: 24px;
  border-radius: 40px;
}

.sideNavbar-wrapper
  .sidebar-footer
  .dark-light-mode-toggle-container
  .dark-light-mode-label {
  color: var(--appLightFontColor);
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.dark-light-mode-toggle input[type="checkbox"] {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: 0;
  cursor: pointer;
  opacity: 0;
  z-index: 2;
}

.dark-light-mode-toggle span.label-animation {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  overflow: hidden;
  opacity: 1;
  background-color: #fff;
  /*border: 1px solid var(--appLightFontColor);*/
  /*box-shadow: 0px 2px 25px var(--appLightFontColor);*/
  box-shadow: 0 0 5px var(--appLightFontColor);
  border-radius: 20px;
  transition: 0.2s ease background-color, 0.2s ease opacity;
}

.dark-light-mode-toggle span.label-animation:before,
.dark-light-mode-toggle span.label-animation:after {
  content: "";
  position: absolute;
  top: 2px;
  width: 20px;
  height: 20px;
  border-radius: 50%;
  transition: 0.5s ease transform, 0.2s ease background-color;
}

.dark-light-mode-toggle span.label-animation:before {
  background-color: #fff;
  transform: translate(-27px, 0px);
  z-index: 1;
}

.dark-light-mode-toggle span.label-animation:after {
  background-color: goldenrod;
  transform: translate(2px, 0px);
  z-index: 0;
}

.dark-light-mode-toggle input[type="checkbox"]:checked + span.label-animation {
  background-color: #000;
}

.dark-light-mode-toggle input[type="checkbox"]:active + span.label-animation {
  opacity: 0.5;
}

.dark-light-mode-toggle
  input[type="checkbox"]:checked
  + span.label-animation:before {
  background-color: #000;
  transform: translate(24px, -2px);
}

.dark-light-mode-toggle
  input[type="checkbox"]:checked
  + span.label-animation:after {
  background-color: #fff;
  transform: translate(27px, 0px);
}

.dark-light-mode-toggle.w-36px {
  width: 36px;
}

.dark-light-mode-toggle.w-36px span.label-animation:before,
.dark-light-mode-toggle.w-36px span.label-animation:after {
  width: 20px;
  height: 20px;
}

.dark-light-mode-toggle.w-36px
  input[type="checkbox"]:checked
  + span.label-animation:before {
  transform: translate(10px, -2px);
}

.dark-light-mode-toggle.w-36px
  input[type="checkbox"]:checked
  + span.label-animation:after {
  transform: translate(13px, 0px);
}

